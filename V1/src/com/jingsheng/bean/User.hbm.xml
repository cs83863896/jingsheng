<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jingsheng.bean.User" table="USER" >
		<id name="userId" type="java.lang.Integer" column="userId">
			<generator class="increment" />
		</id>
		<property name="userName" type="java.lang.String" column="userName" length="30"/>
		<property name="userPwd" type="java.lang.String" column="userPwd" length="30"/>
		<property name="userType" type="java.lang.String" column="userType" length="10"/>
		<property name="userCreateUser" type="java.lang.String" column="userCreateUser" length="30" />
		<property name="userCreateDate" type="java.lang.String" column="userCreateDate" />
		<property name="userUpdUser" type="java.lang.String" column="userUpdUser" length="30" />
		<property name="userUpdDate" type="java.lang.String" column="userUpdDate" />
	</class>
</hibernate-mapping>