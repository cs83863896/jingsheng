<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jingsheng.bean.Vendor" table="Vendor" >
		<id name="vendorId" type="java.lang.Integer" column="vendorId">
			<generator class="increment" />
		</id>
		<property name="vendorName" type="java.lang.String" column="vendorName" length="30"/>
		<property name="vendorCategoryId" type="java.lang.Integer" column="vendorCategoryId" length="11"/>
		<property name="vendorCreateUser" type="java.lang.String" column="vendorCreateUser" length="30" />
		<property name="vendorCreateDate" type="java.lang.String" column="vendorCreateDate" />
		<property name="vendorUpdUser" type="java.lang.String" column="vendorUpdUser" length="30" />
		<property name="vendorUpdDate" type="java.lang.String" column="vendorUpdDate" />
	</class>
</hibernate-mapping>