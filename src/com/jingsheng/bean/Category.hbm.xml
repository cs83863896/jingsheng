<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jingsheng.bean.Category" table="CATEGORY" >
		<id name="categoryId" type="java.lang.Integer" column="categoryId">
			<generator class="increment" />
		</id>
		<property name="categoryName" type="java.lang.String" column="categoryName" length="30"/>
		<property name="categoryCreateUser" type="java.lang.String" column="categoryCreateUser" length="30" />
		<property name="categoryCreateDate" type="java.lang.String" column="categoryCreateDate" />
		<property name="categoryUpdUser" type="java.lang.String" column="categoryUpdUser" length="30" />
		<property name="categoryUpdDate" type="java.lang.String" column="categoryUpdDate" />
	</class>
</hibernate-mapping>